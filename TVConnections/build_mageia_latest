#!/bin/bash
date +%F_%H-%M-%S
mv TVDatabase/postgresql $HOME/tmp
source $HOME/.cache/envTiledViz
sed -e "s&_NTP_&$NTPserver&"  -e "s&_SMTP_&$SMTPserver&" TVConnections/mageianvidia/Dockerfile_i > TVConnections/mageianvidia/Dockerfile
docker build -t mageianvidia:latest -f TVConnections/mageianvidia/Dockerfile .
mv $HOME/tmp/postgresql  TVDatabase
date +%F_%H-%M-%S
